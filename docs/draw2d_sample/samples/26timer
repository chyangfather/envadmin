<script>
  var workflow  = new draw2d.Workflow("paintarea");


  // Add my timer figure to the canvas.
  //
  var obj = new draw2d.MyTimerFigure(1000);
  workflow.addFigure(obj, 200,300);

  obj = new draw2d.MyTimerFigure(800);
  workflow.addFigure(obj, 230,350);

  obj = new draw2d.MyTimerFigure(1500);
  workflow.addFigure(obj, 300,300);


</script>


