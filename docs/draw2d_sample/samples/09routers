<script>
  // Create an interactive Canvas
  //
  draw2d.Connection.setDefaultRouter(new draw2d.FanConnectionRouter());
  var workflow  = new draw2d.Workflow("paintarea");

  // Add an simple annotation to the canvas
  //
  var annotation = new draw2d.Annotation("NOTE: Drag&Drop the red port to the blue port to create a connection. Use the connection context menu to switch the router implementation.");
  annotation.setDimension(300,70);
  workflow.addFigure(annotation,100,200);

  var annotation = new draw2d.Annotation("NOTE: Inspect ContextmenuConntection.js to find the 'secret'.");
  annotation.setDimension(300,70);
  workflow.addFigure(annotation,100,300);


  // Add the start node to the canvas
  //
  var startObj = new draw2d.Start();
  workflow.addFigure(startObj, 450,300);

  // Add the end node to the canvas
  //
  var endObj   = new draw2d.End();
  workflow.addFigure(endObj,600,350);

  // create a new connection
  var c = new draw2d.ContextmenuConnection();
  c.setSource(startObj.outputPort);
  c.setTarget(endObj.inputPort);
  workflow.addFigure(c);

  // create a new connection
  c = new draw2d.ContextmenuConnection();
  c.setSource(startObj.outputPort);
  c.setTarget(endObj.inputPort);
  workflow.addFigure(c);

  // create a new connection
  c = new draw2d.ContextmenuConnection();
  c.setRouter(new draw2d.ManhattanConnectionRouter());
  c.setSource(startObj.outputPort3);
  c.setTarget(endObj.inputPort4);

  workflow.addFigure(c);


</script>


