<script>
  // Create the interactive paint area
  // (use "Canvas" for the non interactive paint area)
  //
  var workflow  = new draw2d.Workflow("paintarea");

  // Add an annotation to the workflow area
  //
  var annotation = new draw2d.Annotation("NOTE: The arrow figure overrides the paint() method of the Line class.");
  annotation.setDimension(250,70);
  workflow.addFigure(annotation,200,200);


  // add my complex line figure
  //
  var line= new draw2d.ArrowLine();
  line.setStartPoint(100,180);
  line.setEndPoint(130,300);
  workflow.addFigure(line);


</script>


